{
  "name": "ng-go-to-top-chrome-extension",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all build:dev copy-background:dev copy-content-script:dev watch-extension",
    "watch-extension": "node extension-watcher.js",
    "build:dev": "ng build --configuration development",
    "build:prod": "ng build",
    "copy-background:dev": "node copy-background.js dev",
    "copy-background:prod": "node copy-background.js prod",
    "copy-content-script:dev": "node copy-content-script.js dev",
    "copy-content-script:prod": "node copy-content-script.js prod",
    "zip-extension": "node zip-extension.js",
    "build:extension": "npm-run-all build:prod copy-background:prod copy-content-script:prod zip-extension",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/forms": "^17.0.4",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.0.4",
    "@angular/router": "^17.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "ws": "^8.14.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular/cli": "~17.0.3",
    "@angular/compiler-cli": "^17.0.4",
    "@types/jasmine": "~4.3.0",
    "archiver": "^6.0.1",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.2.2"
  }
}
